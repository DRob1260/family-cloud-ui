{
  "overwrite":  true,
  "schema": "http://localhost:5000/schema",
  "documents": "src/**/*.graphql",
  "generates": {
    "src/graphql/generated/types.ts": {
      "plugins": [
        "typescript",
        "typescript-operations",
        "typescript-react-query"
      ],
      "config": {
        "fetcher": "graphql-request"
      }
    }
  }
}
